

public class RegularExpress 
{
	public static void main(String[] args) 
	{
		String s1= "a";
		System.out.println("S1=" + s1);
		boolean match = s1.matches(".");
		System.out.println("Match" + match);
		
		
		String s1= "Nguyen Thanh Phong";
		System.out.println("S1=" +s1);
		boolean match = s1.matches(".*");
		System.out.println("-Match . " + match);
	}
}
